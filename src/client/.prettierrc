{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "strict",
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": ["*.js", "*.ts", "*.json"],
      "options": {
        "singleQuote": false,
        "trailingComma": "none"
      }
    }
  ]
}
